{
  "name": "react-node-starter",
  "version": "1.0.0",
  "description": "an example for deploying a React + NodeJS app to Heroku",
  "main": "server.js",
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "dev": "concurrently -k \"npm run frontend\" \"npm run backend\"",
    "frontend": "webpack -d --watch",
    "backend": "nodemon server.js",
    "start": "npm run build && NODE_ENV=production node server.js",
    "test": "mocha --require babel-core/register ./test/**/*.js --require ignore-styles --exit",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-arrays": "^2.0.0",
    "chai-http": "^3.0.0",
    "connect-mongo": "^2.0.0",
    "css-loader": "^0.28.0",
    "dotenv": "^4.0.0",
    "enzyme": "^2.8.1",
    "expect": "^1.20.2",
    "express": "^4.15.2",
    "express-session": "^1.15.6",
    "express-validator": "^4.2.1",
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "mocha": "^4.0.1",
    "mongodb": "^2.2.31",
    "mongoose": "^4.12.4",
    "morgan": "^1.9.0",
    "node-cron": "^1.2.1",
    "node-sass": "^4.5.2",
    "nodemailer": "^4.2.0",
    "nodemailer-sendgrid": "^1.0.1",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.6.0",
    "react": "^15.5.4",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^15.5.4",
    "react-instafeed": "^0.4.0",
    "react-redux": "^5.0.5",
    "react-redux-form": "^1.16.1",

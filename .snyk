# Snyk (https://snyk.io) policy file, patches or ignores known vulnerabilities.
version: v1.8.0
# ignores vulnerabilities until expiry date; change duration by modifying expiry date
ignore:
  'npm:superagent:20170807':
    - chai-http > superagent:
        reason: None given
        expires: '2017-12-25T21:05:27.654Z'
  'npm:tunnel-agent:20170305':
    - node-sass > request > tunnel-agent:
        reason: None given
        expires: '2017-12-25T21:05:27.654Z'
# patches apply the minimum changes required to fix a vulnerability
patch:
  'npm:minimatch:20160620':
    - is-valid-date > tape > glob > minimatch:
        patched: '2017-11-28T21:56:59.916Z'
    - is-valid-zip > tape > glob > minimatch:
        patched: '2017-11-28T21:56:59.916Z'
